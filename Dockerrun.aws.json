{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "api",
      "image": "537041990806.dkr.ecr.ap-northeast-2.amazonaws.com/test-api",
      "hostname": "api",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5000
        }
      ]
    },
    {
      "name": "nginx",
      "image": "537041990806.dkr.ecr.ap-northeast-2.amazonaws.com/test-nginx",
      "hostname": "nginx",
      "essential": true,
      "memory": 64,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["auth", "api"]
    }
  ]
}
